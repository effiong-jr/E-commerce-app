ntrol>\r\n                  </Col>\r\n                  <Col>\r\n                    <Button\r\n                      type='button'\r\n                      variant='light'\r\n                      onClick={() => removeFromCartHandler(item.product)}\r\n                    >\r\n                      <i className='fas fa-trash'></i>\r\n                    </Button>\r\n                  </Col>\r\n                </Row>\r\n              </ListGroup.Item>\r\n            ))}\r\n          </ListGroup>\r\n        )}\r\n      </Col>\r\n      <Col md={4}>\r\n        <Card>\r\n          <ListGroup>\r\n            <ListGroup.Item variant='flush'>\r\n              <h2>\r\n                Subtotal ({cartItems.reduce((acc, item) => acc + item.qty, 0)})\r\n                items\r\n              </h2>\r\n              ${' '}\r\n              {cartItems\r\n                .reduce((acc, item) => acc + item.qty * item.price, 0)\r\n                .toFixed(2)}\r\n            </ListGroup.Item>\r\n\r\n            <ListGroup.Item>\r\n              <Button\r\n                type='button'\r\n                className='btn-block'\r\n                onClick={checkoutHandler}\r\n                disabled={cartItems.length === 0}\r\n              >\r\n                Proceed To Checkout\r\n              </Button>\r\n            </ListGroup.Item>\r\n          </ListGroup>\r\n        </Card>\r\n      </Col>\r\n    </Row>\r\n  )\r\n}\r\n\r\nexport default CartScreen\r\n","C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\screens\\ProductScreen.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\reducers\\cartReducers.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\reducers\\productReducers.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\components\\Message.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\compone