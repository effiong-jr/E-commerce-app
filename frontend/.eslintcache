ore.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\components\\Header.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\components\\Footer.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\screens\\HomeScreen.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\screens\\CartScreen.js",["102"],"import { useState, useEffect } from 'react'\r\nimport { Link } from 'react-router-dom'\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport {\r\n  Row,\r\n  Col,\r\n  Button,\r\n  Card,\r\n  ListGroup,\r\n  Image,\r\n  FormControl,\r\n} from 'react-bootstrap'\r\nimport { addToCart, removeFromCart } from '../actions/cartActions'\r\nimport Message from '../components/Message'\r\n\r\nconst CartScreen = ({ match, location, history }) => {\r\n  const productId = match.params.id\r\n  const qty = location.search\r\n    ? Number(new URLSearchParams(location.search).get('qty'))\r\n    : 1\r\n\r\n  const dispatch = useDispatch()\r\n  const cart = useSelector((state) => state.cart)\r\n  const { cartItems } = cart\r\n\r\n  useEffect(() => {\r\n    if (productId) {\r\n      dispatch(addToCart(productId, qty))\r\n    }\r\n  }, [dispatch, productId, qty])\r\n\r\n  const removeFromCartHandler = (id) => {\r\n    dispatch(removeFromCart(id))\r\n  }\r\n\r\n  const checkoutHandler = () => {\r\n    history.push('/login?redirect=shipping')\r\n  }\r\n\r\n  return (\r\n    <Row>\r\n      <Col md={8}>\r\n        <h1>Shopping Cart</h1>\r\n        {cartItems.length === 0 ? (\r\n          <Message>\r\n            Your cart is empty! <Link to='/'>Go Back</Link>\r\n          </Message>\r\n        ) : (\r\n          <ListGroup variant='flush'>\r\n            {cartItems.map((item) => (\r\n              <ListGroup.Item key={item.product}>\r\n                <Row>\r\n                  <Col md={2}>\r\n                    <Image src={item.image} alt={item.name} fluid rounded />\r\n                  </Col>\r\n                  <Col md={3}>\r\n                    <Link to={`/products/${item.product}`}>{item.name}</Link>\r\n                  </Col>\r\n                  <Col md={2}>${item.price}</Col>\r\n                  <Col md={2}>\r\n                    <FormControl\r\n                      as='select'\r\n                      value={item.qty}\r\n                      className='px-3'\r\n                      onChange={(e) =>\r\n                        dispatch(\r\n                          addToCart(item.product, Number(e.target.value))\r\n                        )\r\n                      }\r\n                    >\r\n                      {[...Array(item.countInStock).keys()].map((x) => (\r\n                        <option key={x + 1} value={x + 1}>\r\n                          {x + 1}\r\n                        </option>\r\n                      ))}\r\n                    </FormControl>\r\n                  </Col>\r\n                  <Col>\r\n                    <Button\r\n                      type='button'\r\n                      variant='light'\r\n                      onClick={() => removeFromCartHandler(item.product)}\r\n                    >\r\n                      <i className='fas fa-trash'></i>\r\n                    </Button>\r\n                  </Col>\r\n                </Row>\r\n              </ListGroup.Item>\r\n            ))}\r\n          </ListGroup>\r\n        )}\r\n      </Col>\r\n      <Col md={4}>\r\n        <Card>\r\n          <ListGroup>\r\n            <ListGroup.Item variant='flush'>\r\n              <h2>\r\n                Subtotal ({cartItems.reduce((acc, item) => acc + item.qty, 0)})\r\n                items\r\n              </h2>\r\n              ${' '}\r\n              {cartItems\r\n                .reduce((acc, item) => acc + item.qty * item.price, 0)\r\n                .toFixed(2)}\r\n            </ListGroup.Item>\r\n\r\n            <ListGroup.Item>\r\n              <Button\r\n                type='button'\r\n                className='btn-block'\r\n                onClick={checkoutHandler}\r\n                disabled={cartItems.length === 0}\r\n              >\r\n                Proceed To Checkout\r\n              </Button>\r\n            </ListGroup.Item>\r\n          </ListGroup>\r\n        </Card>\r\n      </Col>\r\n    </Row>\r\n  )\r\n}\r\n\r\nexport default CartScreen\r\n","C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\screens\\ProductScreen.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\reducers\\cartReducers.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\reducers\\productReducers.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\components\\Message.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\components\\Loader.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\components\\Product.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\actions\\productActions.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\components\\Rating.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\actions\\cartActions.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\constants\\productConstants.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\reducers\\userLoginReducers.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\constants\\userConstants.js",[],"C:\\Users\\effiongge\\Documents\\personal\\study\\E-commerce-app\\frontend\\src\\screens\\LoginScreen.js",["103","104",